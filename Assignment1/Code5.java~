import java.util.*;

public class Code5
{
	public static void main(String []args)
	{
		int n;
		Scanner input = new Scanner(System.in);
		n = input.nextInt();
		int arr[];
		arr = new int[n];
		for(int i=0;i<n;++i)
			arr[i] = input.nextInt();
		for(int i=0;i<n;++i)
		{
			int j,currentidx = arr[i];
			for(j = i-1;j>=0;--j)
			{
				if(arr[j] > currentidx)
					arr[j+1] = arr[j];
				else break;
			}
			arr[j+1] = currentidx;
			System.out.println(Arrays.toString(arr));
		}
		System.out.println(Arrays.toString(arr));
	}
}
